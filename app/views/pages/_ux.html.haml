	%form.form_login.form-pos{action: "#"}
		%div
			
			.form-group
				.input_wrap
					%input.inp_outlinenone#disabledTextInput.form-control{type: "text"}
					%span.form_inp-text.form_inp-default
						Имя
						%span.sprite.i-form_inp-star
					%span.form-hint_text Имя состоит из букв
			.form-group.has-success
				.input_wrap
					%input.inp_outlinenone#inputSuccess1.form-control{type: "text"}
					%span.form_inp-text 
						Василий
					%span.form-hint_text Имя состоит из букв
			.form-group.has-error
				.input_wrap
					%input.inp_outlinenone#inputWarning1.form-control{type: "text"}
					%span.form_inp-text 
						111
				%label.control-label{for: "inputWarning1"} Имя состоит из букв
			.form-group.has-error.submit-group
				.input_wrap
					%input.inp_outlinenone#inputWarning1.form-control{type: "text"}
					%span.form_inp-text 
						111
				%button.btn.btn-default.link_button{type: "submit"} Отправить
				%label.control-label{for: "inputWarning1"} Введите e-mail
			.form-group.has-error.submit-group
				.input_wrap
					%input.inp_outlinenone#inputWarning1.form-control{type: "text"}
					%span.form_inp-text 
						111
				%button.btn.btn-default.link_button{type: "submit"} Отправить
				%label.control-label{for: "inputWarning1"} Введите e-mail
			%ul.reset_style.form-style_list
				%li
					.input-group.find-block_wrap
						%input.inp_outlinenone.form-control.find-block{placeholder: "Введите адрес, чтобы найти муниципалитет", type: "text"}
						%span.input-group-btn
							%button.btn.btn-default{type: "button"} Найти
				%li
					.input-group.find-block_wrap
						%input.inp_outlinenone.form-control.find-block{placeholder: "Введите адрес, чтобы найти муниципалитет", value: "Димитрова ул. 65", type: "text"}
